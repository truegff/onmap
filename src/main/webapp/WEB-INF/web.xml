<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>ge.lanmaster.onmap.root.root login</servlet-name>
        <servlet-class>ge.lanmaster.onmap.root.server.LoginServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ge.lanmaster.onmap.root.root login</servlet-name>
        <url-pattern>/root/login</url-pattern>
    </servlet-mapping>

    <!-- Servlets -->

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>root.html</welcome-file>
    </welcome-file-list>

</web-app>
