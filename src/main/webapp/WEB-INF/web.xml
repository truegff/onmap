<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
    <servlet>
        <servlet-name>userService</servlet-name>
        <servlet-class>ge.lanmaster.onmap.server.MyUserServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>userService</servlet-name>
        <url-pattern>/OnMap/userService</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>externalService</servlet-name>
        <servlet-class>ge.lanmaster.gwt.ui.widgets.server.MyExternalServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>externalService</servlet-name>
        <url-pattern>/OnMap/externalService</url-pattern>
    </servlet-mapping>

  <!-- RemoteApi -->

    <servlet>
        <servlet-name>RemoteApi</servlet-name>
        <servlet-class>com.google.apphosting.utils.remoteapi.RemoteApiServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RemoteApi</servlet-name>
        <url-pattern>/remote_api</url-pattern>
    </servlet-mapping>

  <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>OnMap.html</welcome-file>
    </welcome-file-list>

</web-app>
