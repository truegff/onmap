<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='root'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name="com.google.gwt.user.User"/>

    <!-- We need the JUnit module in the main module,               -->
    <!-- otherwise eclipse complains (Google plugin bug?)           -->
    <!--<inherits name='com.google.gwt.junit.JUnit'/>-->

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <!--<inherits name='com.google.gwt.user.theme.chrome.Chrome'/>-->
    <!--<inherits name='com.google.gwt.user.theme.dark.Dark'/>-->

    <!-- Other module inherits                                      -->
    <!--<inherits name="com.extjs.gxt.ui.GXT"/>-->
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.maps.GoogleMaps"/>
    <inherits name="com.googlecode.objectify.Objectify" />
    <!--<inherits name="ge.lanmaster.onmap.demo1.Demo1" />-->


    <set-property name="user.agent" value="safari"/>

    <!-- Use ClientFactoryImpl by default -->
    <replace-with class="ge.lanmaster.onmap.root.client.ClientFactoryImpl">
        <when-type-is class="ge.lanmaster.onmap.root.client.ClientFactory"/>
    </replace-with>

    <!-- Specify the app entry point class.                         -->
    <entry-point class='ge.lanmaster.onmap.root.client.root'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>


    <servlet path="/login" class="ge.lanmaster.onmap.root.server.LoginServiceImpl"/>

</module>
