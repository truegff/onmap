<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .alignRight {
            position: absolute;
            right: 10px;
            top: 10px;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{style.alignRight}">
            <g:Anchor ui:field="loginLink" text="Log In"></g:Anchor>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>
